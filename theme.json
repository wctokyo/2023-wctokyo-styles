                },
                {
                    "slug": "wc23-black",
                    "color": "#231815",
                    "name": "夜の帳"
                },
                {
                    "slug": "wc23-milk",
                    "color": "#F3EEE8",
                    "name": "ミルク"
                },
                {
                    "slug": "wc23-azure",
                    "color": "#4041E4",
