                }
            ]
        },
        "layout": {
            "contentSize": "920px",
            "wideSize": "1130px"
        },
        "typography": {
            "fontFamilies": [
                {
                    "fontFamily": "'Noto Sans JP', sans-serif",
                    "slug": "noto-sans",
                    "name": "Noto Sans JP"
                },
                {
                    "fontFamily": "'Josefin Sans', sans-serif",
                    "slug": "josefin-font",
                    "name": "Josefin Sans"
                }
            ],
            "fontSizes": [
                {
                    "slug": "small",
