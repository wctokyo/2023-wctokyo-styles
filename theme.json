            "dropCap": false,
            "customFontSize": true,
            "fluid": false,
            "lineHeight": true,
            "textColumns": true,
            "textTransform": true,
            "textDecoration": true,
            "fontWeight": true,
            "fontStyle": true,
            "letterSpacing": true
